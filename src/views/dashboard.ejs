<div class="card shadow mb-4">
  <div class="card-header py-3">
    <a
      id="add"
      href="/bread/form"
      class="modal-open btn btn-primary btn-round"
      type="submit"
      data-placement="bottom"
      title="Tambah Data"
    >
      <i class="bx bxs-file-plus"></i> Tambah Data
    </a>
  </div>
  <div class="card-body">
    <div class="mt-3">
      <table
        class="table table-striped"
        style="width: 100%"
        id="dataTable"
        url="/product/table"
        edit="true"
        delete="true"
      >
        <thead>
          <tr>
            <th key="sku" type="string">No Barang</th>
            <th key="name">Nama</th>
            <th key="buyPrice" type="string">Harga Beli</th>
            <th key="currentPrice" type="string">Harga Jual</th>
            <th key="stock" type="string">Stok Saat Ini</th>
          </tr>
        </thead>
      </table>
    </div>
  </div>
</div>
